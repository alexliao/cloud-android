<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="buttonBar">
		<item name="android:background">?colorPanel</item>
	</style>
	<style name="btn">
		<item name="android:background">@drawable/btn</item>
		<item name="android:textColor">@color/btn</item>
		<item name="android:textSize">18sp</item>
		<item name="android:padding">9dip</item>
	</style>
	<style name="action_row">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">65dip</item>
		<item name="android:background">@android:drawable/menuitem_background</item>
		<item name="android:padding">10dip</item>
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">?colorAction</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:drawablePadding">10dip</item>
	</style>
	<style name="action_divider">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">1px</item>
		<item name="android:background">?colorDivider</item>
	</style>
	<style name="page_title">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/gray</item>
	</style>

</resources>